<h2>登録済みルーム</h2>
<div>
    <table>
        <thead>
            <tr>
                <th>ルーム画像</th>
                <th>ルーム名</th>
                <th>ルーム紹介</th>
                <th>料金</th>
                <th>作成日</th>
            </tr>
        </thead>
        <tbody>
            <% unless @inns == nil %>
                <% @inns.each do |inn| %>
                    <tr>
                        <td><%= image_tag (inn.image.thumb.url) %></td>
                        <td><%= link_to inn.name, inn%></td>
                        <td><%= inn.introduction %></td>
                        <td><%= inn.price %></td>
                        <td><%= inn.created_at%></td>
                    <% end %>
                </tr>
            <% end %>
        </tbody>
    </table>
</div>
